{
  "name": "nuxt-simple-robots",
  "type": "module",
  "version": "4.0.0-rc.2",
  "packageManager": "pnpm@8.12.1",
  "description": "Tame the robots crawling and indexing your Nuxt site with ease.",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/robots",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-simple-robots.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-simple-robots/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./util": {
      "types": "./dist/runtime/util.d.ts",
      "import": "./dist/runtime/util.mjs"
    },
    "./dist/runtime/util": {
      "types": "./dist/runtime/util.d.ts",
      "import": "./dist/runtime/util.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare .playground",
    "release": "bumpp && pnpm -r publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.0.6",
    "@nuxt/kit": "^3.8.2",
    "defu": "^6.1.3",
    "nuxt-site-config": "^2.1.0",
    "nuxt-site-config-kit": "^2.1.0",
    "pathe": "^1.1.1",
    "sirv": "^2.0.3",
    "ufo": "^1.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.4.6",
    "@nuxt/content": "^2.9.0",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/test-utils": "3.8.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "8.0.0-rc.11",
    "bumpp": "^9.2.1",
    "eslint": "8.56.0",
    "execa": "^8.0.1",
    "nuxt": "^3.8.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "build": {
    "externals": [
      "h3"
    ]
  }
}
