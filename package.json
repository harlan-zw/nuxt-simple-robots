{
  "name": "nuxt-simple-robots",
  "type": "module",
  "version": "3.1.1",
  "packageManager": "pnpm@8.6.12",
  "description": "Tame the robots crawling and indexing your Nuxt site with ease.",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/robots",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-simple-robots.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-simple-robots/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "build": "nuxt-module-build --stub && nuxt-module-build prepare && nuxt-module-build",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare .playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "defu": "^6.1.2",
    "nuxt-site-config": "^1.0.10",
    "nuxt-site-config-kit": "^1.0.10",
    "pathe": "^1.1.1",
    "ufo": "^1.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/test-utils": "3.6.5",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "bumpp": "^9.1.1",
    "eslint": "8.47.0",
    "execa": "^8.0.1",
    "nuxt": "^3.6.5",
    "vitest": "^0.34.1"
  }
}
